{
	"containerDefinitions": [
		{
			"image": "lunchboxdc/pi:$IMAGE_TAG",
			"name": "pi",
			"cpu": 0,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80,
					"protocol": "tcp"
				}
			],
			"environment": [
				{
					"name": "DB_HOST",
					"value": "$PI_PROD_DB_HOST"
				},
				{
					"name": "DB_USER",
					"value": "$PI_PROD_DB_USER"
				},
				{
					"name": "DB_PASS",
					"value": "$PI_PROD_DB_PASS"
				},
				{
					"name": "FB_TOKEN",
					"value": "$PI_PROD_FB_TOKEN"
				}
			],
			"volumesFrom": [],
			"mountPoints": [],
			"essential": true
		}
	],
	"family": "pi-family"
}
